# CELL 5B: Find Your Catalog
from databricks.sdk import WorkspaceClient

w = WorkspaceClient()

print("ðŸ“‹ Available Catalogs:")
catalogs = list(w.catalogs.list())
for cat in catalogs:
    print(f"  - {cat.name}")
    
    # List schemas in each catalog
    try:
        schemas = list(w.schemas.list(catalog_name=cat.name))
        for schema in schemas:
            print(f"      â””â”€ {schema.name}")
    except:
        pass
