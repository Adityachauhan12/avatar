path = '/dbfs/FileStore/VideoReTalking/inference.py'

with open(path, 'r') as f:
    lines = f.readlines()

# Comment out GFPGAN import line
new_lines = []
for line in lines:
    if 'from third_part.GFPGAN.gfpgan import GFPGANer' in line:
        new_lines.append(f'# {line}')  # Comment it out
        print(f"✅ Commented out: {line.strip()}")
    else:
        new_lines.append(line)

with open(path, 'w') as f:
    f.writelines(new_lines)

print("✅ GFPGAN import disabled in inference.py")
