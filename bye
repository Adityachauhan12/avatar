import os
import subprocess

# List what's in the checkpoint directory
result = subprocess.run(
    ["ls", "-lah", "/dbfs/FileStore/ditto_checkpoints/ditto_trt_t4/"],
    capture_output=True,
    text=True
)
print("STDOUT:")
print(result.stdout)
print("\nSTDERR:")
print(result.stderr)
print("\nReturn code:", result.returncode)
