@app.post("/debug-echo")
async def debug_echo(
    audio: UploadFile = File(...),
    image: UploadFile = File(...),
):
    return {
        "audio_field": audio.filename,
        "image_field": image.filename,
        "audio_content_type": audio.content_type,
        "image_content_type": image.content_type,
    }
